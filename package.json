{
  "name": "takopi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "seed:generate": "tsx scripts/seeding/generate.ts",
    "seed:manual": "tsx scripts/seeding/seed-manual.ts",
    "test:connections": "npx tsx scripts/test-connections.ts",
    "test:e2e": "npx playwright test --config=testing/playwright.config.ts",
    "test:e2e:headed": "npx playwright test --headed --config=testing/playwright.config.ts",
    "test:e2e:install": "npx playwright install",
    "diagnostics": "npx tsx scripts/diagnostics/takopi-diagnostics.ts",
    "lint": "eslint"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.19.0",
    "@types/multer": "^2.0.0",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "next": "^15.5.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.5.2",
    "transbank-sdk": "^6.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "^15.5.7",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
